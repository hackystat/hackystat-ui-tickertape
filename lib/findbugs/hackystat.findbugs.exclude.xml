<FindBugsFilter>
  <Match class="~.*\.jaxb\.*" />
  <Match>
    <Package name="~.*\.jaxb" />
  </Match>
  <Match>
    <Or>
      <Package name="~.*\.language.ast" />
      <Package name="~.*\.language.parser" />
      <Package name="~.*\.language.parser.impl" />
      <Package name="~.*\.language" />
    </Or>
  </Match>

  <Match class="~.*\.language\.*" />
  <Match class="org.hackystat.sensorbase.resource.sensordatatypes.SimpleSDTExample" />
  <Match>
    <Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING" />
  </Match>
  <Match>
    <Bug pattern="MS_CANNOT_BE_FINAL" />
  </Match>
</FindBugsFilter>

